        <joint name="${prefix}gripper_finger1_joint" type="prismatic"> <!-- left part -->
            <parent link="${prefix}gripper_base_link"/>
            <child link="${prefix}gripper_finger1_knuckle_link"/>
            <axis xyz="0 0 1"/>
            <!--origin xyz="-0.05 0.06 0.065 " rpy="-${M_PI/2} 0 -${M_PI/2}"/-->
            <origin xyz="-0.01 0 0.105" rpy="0 ${M_PI/2} ${M_PI}"/>
            <limit effort="500" velocity="0.02" lower="-0.25" upper="0.1" />
            <axis xyz="0 0 1"/>
        </joint>

        <joint name="${prefix}gripper_finger2_joint" type="prismatic"> <!-- right part -->
            <parent link="${prefix}gripper_base_link"/>
            <child link="${prefix}gripper_finger2_knuckle_link"/>
            <axis xyz="0 0 1"/>
            <!--origin xyz="0.05 0.06 0.065 " rpy="-${M_PI/2} ${M_PI} ${M_PI/2}"/-->
            <origin xyz="0.01 0 0.105" rpy="-${M_PI} ${M_PI/2} -${M_PI}"/>
            <limit lower="-0.25" upper="1.0" velocity="0.02" effort="500"/>
            <mimic joint="${prefix}gripper_finger1_joint"/>
        </joint>

        <link name="${prefix}gripper_finger1_knuckle_link">
