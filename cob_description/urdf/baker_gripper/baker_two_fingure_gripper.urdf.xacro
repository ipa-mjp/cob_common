    <xacro:macro name="baker_gripper" params="prefix parent *origin">

        <joint name="${prefix}gripper_base_joint" type="fixed">
            <parent link="${parent}"/>
            <child link="${prefix}gripper_base_link"/>
            <xacro:insert_block name="origin"/>
        </joint>
        <link name="${prefix}gripper_base_link">
        <visual>
            <xacro:sphere_geometry radius="0.00001"/>
            <material name="IPA/LightGrey" />
        </visual>
        <collision>
            <xacro:sphere_geometry radius="0.000001"/>
        </collision>
        </link>

